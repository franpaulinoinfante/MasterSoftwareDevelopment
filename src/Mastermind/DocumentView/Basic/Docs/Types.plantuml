@startuml MastermindTypes
title Mastermind Types
namespace Types{
    enum Color{
        +Red,
        +Green,
        +Yellow,
        +Blue,
        +Magenta,
        +Cyan,
        +Null
        +IsNull(Color) : bool
        +GetInitialChar(Color) : char
    }

    enum Error{
        +Duplicated,
        +WrongCharacters,
        +WrongLength,
        +Null
        +IsNull(Error) : bool
    }
}
@enduml